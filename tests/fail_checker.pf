goal test1: A /\ (A => B) => B
proof
  [ A /\ (A => B) :
    A;
    A => B;
    B ]
end.

goal test2: A => B => A
proof
  [ A : [ B : A];
  		 B => A];
    (A => B) => A
end.

goal test3: (p \/ q => r) => (p => r) /\ (q => r)
proof
  [ p\/q=>r : 
  	[ p :
  	  p\/q;
  	  r ];
  	p => r;
  	[ q :
  	  r ];
  	q => r;
  	(p => r) /\ (q => r) ];
  (p \/ q => r) => (p => r) /\ (q => r)
end.

goal eq: (p /\ q => r) <=> (p => q => r)
proof
  [ p /\ q => r :
    [ p :
      [ q :
        p /\ q;
        r];
      q => r];
    p => q => r];
  (p /\ q => r) => (p => q => r);
  [ p => q => r :
    [ p /\ q :
      p;
      q;
      q => r;
      r ];
    p /\ q => r];
  (p => q => r) => (p /\ q => r)
end.

goal negimp: (p => q) /\ ~q => ~p
proof
  [ (p => q) /\ ~q :
    p => q;
    ~q;
    q => F;
    [ p :
      q ];
    p => F;
    ~p ];
  (p => q) /\ ~q => ~p
end.

goal morgan: ~(p /\ q) => ~q \/ ~p
proof
  [ ~(p /\ q) :
    [ ~(~q \/ ~p) :
      [ ~q :
        ~q \/ ~p;
        F];
      ~q => F;
      q;
      [ ~p :
        ~q \/ ~p;
        F];
      ~p => F;
      p;
      p /\ q;
      F];
    ~(~q \/ ~p) => F;
    ~~(~q \/ ~p);
    ~~(~q \/ ~p);
    ~~(~q \/ ~p);
    ~~(~p \/ ~q);
    ~q \/ ~p ];
  ~(p /\ q) => ~q \/ ~p
end.